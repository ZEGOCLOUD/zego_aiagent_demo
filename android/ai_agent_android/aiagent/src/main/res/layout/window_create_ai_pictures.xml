<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#66000000">

<!--    <View-->
<!--        android:id="@+id/empty_panel"-->
<!--        android:layout_width="match_parent"-->
<!--        android:layout_height="0dp"-->
<!--        app:layout_constraintTop_toTopOf="parent"-->
<!--        app:layout_constraintBottom_toTopOf="@+id/valid_panel"/>-->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/valid_panel_pics"
        android:layout_width="match_parent"
        android:layout_height="152dp"
        android:background="@drawable/rounded_tts_settings_panel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/create_ai_pics_select"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:text="@string/select_from_pics"
            android:textColor="#000000"
            android:textSize="16sp"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/create_ai_pics_cancel"
            android:layout_width="327dp"
            android:layout_height="48dp"
            android:layout_marginTop="12dp"
            android:text="@string/cancel"
            android:textColor="#000000"
            android:textSize="15sp"
            android:gravity="center"
            android:background="@drawable/rounded_ai_white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/create_ai_pics_select" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>